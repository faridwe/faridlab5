public class MyFifthApp {
    public static void main(String[] args) {
        printCharacter('*', 5, 3);
    }

    public static void printCharacter(char c, int times, int lines) {
        for (int i = 0; i < lines; i++) {
            for (int j = 0; j < times; j++) {
                System.out.print(c);
            }
            System.out.println();
        }
    }
}
